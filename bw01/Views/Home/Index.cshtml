@using DataRepository
@model List<MainHeading>
@{
    ViewBag.Title = "Boxer";
}
<div class="quotation_div">
    <div class="overlay">

        <p>Boxer</p>
        @foreach (MainHeading item in Model)
        {
            <p>@item.Heading</p>
        }
    </div>
</div>               

<footer id="footer">
    <div>@typeof(bw01.MvcApplication).Assembly.GetName().Version  &copy; @DateTime.Now.Year - Boxer - grown men, doing stupid things, expensively!   </div>
</footer>



